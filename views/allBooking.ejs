<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>Sangam Hotels | All Booking</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Favicon -->
    <link rel="apple-touch-icon" sizes="180x180" href="/favicon_io/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon_io/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon_io/favicon-16x16.png">
    <link rel="manifest" href="/favicon_io/site.webmanifest">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Rajdhani&display=swap" rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="lib/animate/animate.min.css" rel="stylesheet">
    <link href="lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="lib/tempusdominus/css/tempusdominus-bootstrap-4.min.css" rel="stylesheet" />

    <!-- Customized Bootstrap Stylesheet -->
    <link href="css/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="css/style.css" rel="stylesheet">
    <link href="css/allBooking.css" rel="stylesheet">
</head>

<body>
    <!-- Spinner Start -->
    <div id="spinner"
        class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
        <div class="spinner-border text-primary" style="width: 3rem; height: 3rem;" role="status">
            <span class="sr-only">Loading...</span>
        </div>
    </div>
    <!-- Spinner End -->


    <!-- Navbar & Hero Start -->
    <div class="container-fluid position-relative p-0">
        <nav class="navbar navbar-expand-lg navbar-light px-4 px-lg-5 py-3 py-lg-0">
            <a href="/" class="navbar-brand p-0">
                <h2 class="text-primary m-0"><i class="fa fa-map-marker-alt me-3"></i>Sangam Hotels</h2>
            </a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
                <span class="fa fa-bars"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarCollapse">
                <div class="navbar-nav ms-auto py-0">
                    <a href="/" class="nav-item nav-link">Home</a>
                    <a href="/cheakIn" class="nav-item nav-link ">Check In</a>
                    <div class="nav-item dropdown">
                        <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">Features</a>
                        <div class="dropdown-menu m-0">
                            <a href="/coupons" class="dropdown-item">Coupons</a>
                        </div>
                    </div>
                    <a href="/allBooking" class="nav-item nav-link active">All Booking</a>
                </div>
                <a href="/logout" class="btn btn-primary rounded-pill py-2 px-4">Logout</a>
            </div>
        </nav>

        <div class="container-fluid bg-primary mb-5 hero-header"
            style="height: 50vh; box-sizing: border-box; display: flex; align-items: center;">
            <div class="container py-5">
                <div class="row justify-content-center py-5">
                    <div class="col-lg-10 pt-lg-5 mt-lg-5 text-center">
                        <h1 class="display-3 text-white mb-3 animated slideInDown"><span style="color: red;">All
                                Booking</span>
                        </h1>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Navbar & Hero End -->

    <div class="main_booking">
        <div class="searchBar">
            <div class="search">
                <input onkeyup="this.onchange()" type="text" name="username/email" id="username/email"
                    placeholder="Search">

                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-search"
                    viewBox="0 0 16 16">
                    <path
                        d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001c.03.04.062.078.098.115l3.85 3.85a1 1 0 0 0 1.415-1.414l-3.85-3.85a1.007 1.007 0 0 0-.115-.1zM12 6.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0z" />
                </svg>
            </div>
            <div class="filter">
                <select name="sort" id="sort">
                    <option value="1" selected>New First</option>
                    <option value="-1">Old First</option>
                </select>
            </div>
            <div class="filter">
                <select name="cheakin" id="cheakin">
                    <option value="0" selected>All</option>
                    <option value="-1">Active</option>
                    <option value="1">Expired</option>
                </select>
            </div>
            <div class="filter">
                <select name="time" id="time">
                    <option value="0">Choose Time(All)</option>
                    <option value="1">1 Month Old</option>
                    <option value="2">2 Month Old</option>
                    <option value="3">3 Month Old</option>
                    <option value="6">6 Month Old</option>
                    <option value="12">1 Year Old</option>
                    <option value="24">2 Year Old</option>
                    <option value="60">5 Year Old</option>
                </select>
            </div>
        </div>
        <hr>
        <div class="content">
            <div style="overflow-x: auto;">
                <table>
                    <thead>
                        <tr style="    background: #d6b161; color: white;">
                            <th>Payment Time</th>
                            <th>Username</th>
                            <th>Email</th>
                            <th>Check In</th>
                            <th>Total Amount</th>
                            <th>Details</th>
                        </tr>
                    </thead>
                    <tbody id="mytable">
                    </tbody>
                </table>
            </div>
        </div>
    </div>

    <div id="popup" class="popup">
        <div class="popup-content">
            <div class="container-xxl py-5">
                <span style="display: block;color: white; margin: 0 12px;background: red;" id="error_success"></span>
                <div class="container">
                    <div class="booking p-5">
                        <div class="row g-5 align-items-center">
                            <div class="col-md-12">
                                <div id="pop_content">
                                    <p class="data_values">Username <span>test</span> </p>
                                    <p class="data_values">Email <span>test</span> </p>
                                    <p class="data_values">Amount <span>test</span> </p>
                                    <p class="data_values">Discount <span>test</span> </p>
                                    <p class="data_values">Nights <span>test</span> </p>
                                    <p class="data_values">Rooms <span>test</span> </p>
                                    <p class="data_values">Check In <span>test</span> </p>
                                    <p class="data_values">Time <span>test</span> </p>
                                    <p class="data_values">Payment Id <span>test</span> </p>
                                    <p class="data_values">Order Id <span>test</span> </p>
                                    <p class="data_values">Signature<span>test</span> </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Footer Start -->
    <div class="container-fluid bg-dark text-light footer pt-5 mt-5 wow fadeIn" data-wow-delay="0.1s">
        <div class="container">
            <div class="copyright">
                <div class="row">
                    <div class="col-md-6 text-center text-md-start mb-3 mb-md-0">
                        &copy; <a class="border-bottom" href="#">Sangam Hotels</a>, All Right Reserved@2023.
                        Made with <span style="color: red;"><i class="fas fa-heart"></i></span> by <a
                            class="border-bottom" href="https://deepaksuthar40128@gmail.com">Deepak
                            Suthar</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Footer End -->


    <!-- Back to Top -->
    <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>


    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="lib/wow/wow.min.js"></script>
    <script src="lib/easing/easing.min.js"></script>
    <script src="lib/waypoints/waypoints.min.js"></script>
    <script src="lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="lib/tempusdominus/js/moment.min.js"></script>
    <script src="lib/tempusdominus/js/moment-timezone.min.js"></script>
    <script src="lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>

    <!-- Template Javascript -->
    <script src="js/main.js"></script>
    <script src="js/allBooking.js"></script>
</body>

</html>